while True:
    a = input()
    if a =="привет":
        print("прив")
    elif a =="ты бот":
        print("сам бот")
    elif a =="пока":
        print("Ариведерчи")
    elif a =="поко":
        print("о май гад поко, БУМ")
    elif a =="чем ты занимаешся":
        print("с такми как ты разгавариваю")
    elif a =="фортнайт или пабдж":
        print("БАБДЖ")
    else:
        print("pan")
